{
  "name": "ts-commons",
  "version": "1.0.44",
  "description": "common methods for typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "localtest": "npm run clean && tsc -p tsconfig.test.json && nyc mocha dist/test --recursive && npm run clean",
    "pretest": "npm run clean && tsc -p tsconfig.test.json",
    "test": "nyc mocha dist/test --recursive && npm run cover -- --report lcovonly && karma start && npm run analyze:sonar",
    "posttest": "npm run clean",
    "prepublish": "tsc",
    "cover": "tsc -p tsconfig.test.json && istanbul cover ./node_modules/mocha/bin/_mocha dist/test/**/*.test.js --recursive",
    "coveralls": "npm run cover -- --report lcovonly",
    "clean": "del-cli .nyc_output && del-cli dist && del-cli .scannerwork && del-cli .idea && del-cli coverage",
    "build": "npm run clean && cross-env NODE_ENV=production webpack",
    "analyze:sonar": "sonar-scanner -Dsonar.projectKey=wz2cool-github:ts-commons -Dsonar.sources=. -Dsonar.host.url=https://sonarcloud.io -Dsonar.organization=wz2cool-github"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wz2cool/ts-commons.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wz2cool/ts-commons/issues"
  },
  "homepage": "https://github.com/wz2cool/ts-commons#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/chai": "~4.1.3",
    "@types/dateformat": "^1.0.1",
    "@types/mocha": "~5.2.1",
    "@types/sinon": "^17.0.3",
    "awesome-typescript-loader": "~5.2.0",
    "babel-loader": "~8.0.0",
    "browserify": "^16.2.2",
    "chai": "~4.1.2",
    "coveralls": "~3.0.1",
    "cross-env": "~5.2.0",
    "dateformat": "^3.0.3",
    "del-cli": "~1.1.0",
    "dts-generator": "^2.1.0",
    "gulp": "^3.9.1",
    "gulp-browserify": "^0.5.1",
    "gulp-clean": "^0.4.0",
    "gulp-cli": "^2.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^5.0.0-alpha.3",
    "gulp-uglify": "^3.0.1",
    "gulp-util": "^3.0.8",
    "istanbul": "~0.4.5",
    "karma": "^3.0.0",
    "karma-browserify": "^5.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "merge-stream": "^1.0.1",
    "mocha": "~5.2.0",
    "mocha-lcov-reporter": "~1.3.0",
    "nyc": "~15.1.0",
    "rimraf": "^2.6.2",
    "sinon": "^18.0.0",
    "sonarqube-scanner": "^2.4.0",
    "ts-loader": "^4.5.0",
    "tsify": "^4.0.0",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.3.3333",
    "typescript-tslint-plugin": "^0.3.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  }
}
